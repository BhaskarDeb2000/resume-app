{
  "name": "resume",
  "description": "A personal resume app built with React, TypeScript, and Vite.",
  "private": true,
  "license": "MIT",
  "version": "6.0.0",
  "author": "Apostolos Gouvalas",
  "homepage": "https://apogouv.github.io/resume/#",
  "repository": {
    "type": "git",
    "url": "https://github.com/ApoGouv/resume.git"
  },
  "keywords": [
    "resume",
    "react",
    "typescript",
    "vite"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "gen-pdfs": "node ./src/Utils/generatePDF.js",
    "dev:gen-pdfs": "node ./src/Utils/generatePDF.js --runDevServer --port 3333",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "predeploy": "npm run dev:gen-pdfs && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@fontsource/lato": "^5.1.1",
    "@playwright/browser-chromium": "^1.49.1",
    "country-flag-icons": "^1.5.14",
    "playwright": "^1.49.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.3",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-preset-env": "^10.1.3",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.4.2",
    "sharp": "^0.33.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.11",
    "vite-plugin-svgr": "^4.3.0"
  }
}
